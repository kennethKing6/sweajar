    "redux-thunk": "^3.1.0",
    "web-vitals": "^2.1.4",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start": "react-scripts start",
    "test:watch": "npx react-scripts test --watch",
    "build": "react-scripts build",
    "test": "react-scripts test src --colors --coverage",
    "eject": "react-scripts eject",
    "prettier": "npx prettier --write ."
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/out/**",
      "!**/src/*.{svg,ico,css}",
      "!**/extension/**",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/.github/**",
      "!**/.next/**",
      "!**/.vscode/**",
      "!**/public/**",
      "!**/extension/**",
      "!**/*.config.{js,jsx}",
      "!**/lcov-report/**",
      "!**/.env/**",
      "!**/public/**",
      "!**/build/**",
      "!**/package.json/**",
      "!**/package-lock.json/**",
      "!**/reportWebVitals.js/**",
      "!**/setupTests.js/**",
      "!**/src/assets/**"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@testing-library/react": "^14.2.1",
    "babel-jest": "^29.7.0",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "text-encoding": "^0.7.0"
  }
}
