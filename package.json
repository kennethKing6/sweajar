    "@mui/x-charts": "^6.19.3",
    "@reduxjs/toolkit": "^2.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.3.2",
    "firebase": "^10.7.1",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.0",
    "react-bubble-ui": "^1.1.2",
    "react-charts": "^3.0.0-beta.57",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-scripts": "5.0.1",
